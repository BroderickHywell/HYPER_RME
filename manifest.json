{
  "manifest_version": 3,
  "name": "HYPER_RME",
  "description": "A tool is only as powerful as the wielder",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
    }
  },
  "permissions": ["scripting", "activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
